{
  "expo": {
    "name": "spotloc",
    "slug": "spotloc",
    "version": "1.3.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0E11"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "config": {
        "googleMapsApiKey": "AIzaSyAHOFdh76NNIGsxyXaq2JwrNavKp9xAsT4",
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription ": "We are using your app usage data, to more effectively reccommend events"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.spotloc.app",
      "buildNumber": "14",
      "appBuildVersion": "14"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "permissions": ["com.google.android.gms.permission.AD_ID"],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDHL7-YqiQ223tGOKR0IBj5QQAb9nsKgxo"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.spotloc.app",
      "versionCode": 11
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "070c97b5-f8fb-409f-835c-2a07355fd3bd"
      }
    },
    "plugins": ["expo-build-properties"]
  },
  "plugins": [
    [
      "expo-build-properties",
      {
        "android": {
          "enableProguardInReleaseBuilds": true
        }
      }
    ],
    [
      "expo-camera",
      {
        "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
      }
    ]
  ]
}
